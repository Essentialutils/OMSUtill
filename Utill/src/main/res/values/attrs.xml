<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="LoaderViewStyle" format="reference" />

    <declare-styleable name="LoaderView">
        <attr name="Loader_Style">
            <enum name="RotatingPlane" value="0" />
            <enum name="DoubleBounce" value="1" />
            <enum name="Wave" value="2" />
            <enum name="WanderingCubes" value="3" />
            <enum name="Pulse" value="4" />
            <enum name="ChasingDots" value="5" />
            <enum name="ThreeBounce" value="6" />
            <enum name="Circle" value="7" />
            <enum name="CubeGrid" value="8" />
            <enum name="FadingCircle" value="9" />
            <enum name="FoldingCube" value="10" />
            <enum name="RotatingCircle" value="11" />
            <enum name="MultiplePulse" value="12" />
            <enum name="PulseRing" value="13" />
            <enum name="MultiplePulseRing" value="14" />
        </attr>
        <attr name="Loader_Color" format="color" />
    </declare-styleable>
    <declare-styleable name="CountryCodeSelect">
        <attr name="ccs_defaultCode" format="integer" />
        <attr name="ccs_textColor" format="color" />
        <attr name="ccs_backgroundColor" format="color" />
        <attr name="ccs_textSize" format="dimension" />
        <attr name="ccs_textFont" format="string" />
        <attr name="ccs_arrowSize" format="dimension" />
        <attr name="ccs_countryPreference" format="string" />
        <attr name="ccs_defaultNameCode" format="string" />
        <attr name="ccs_hideNameCode" format="boolean" />
        <attr name="ccs_keyboardAutoPopOnSearch" format="boolean" />
        <attr name="ccs_customMasterCountries" format="string" />
        <attr name="ccs_showFlag" format="boolean" />
        <attr name="ccs_showFullName" format="boolean" />
        <attr name="ccs_selectionDialogShowSearch" format="boolean" />
        <attr name="ccs_clickable" format="boolean" />
        <attr name="ccs_hidePhoneCode" format="boolean" />
        <attr name="ccs_enableHint" format="boolean" />
        <attr name="ccs_enablePhoneAutoFormatter" format="boolean" />
        <attr name="ccs_setCountryByTimeZone" format="boolean" />

        <!--For dialog-->
        <attr name="ccs_dialogTextColor" format="color" />
    </declare-styleable>

</resources>